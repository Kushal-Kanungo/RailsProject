<h1 class="page-heading">Articles</h1>

<div class="row">
  
      <% @articles.each do |article| %>
      <div class="col-4">
      <div class=" article-list-item d-flex justify-content-between align-items-center"><%= link_to article.title , article %> <%= article.created_at.strftime("%b %d, %y")%> </div>
      </div>
      <% end %>
      <%= render 'add_btn' %>
</div>
